  "keywords": [
    "async",
    "await",
    "cli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "nyc": {
    "include": [
      "src/",
      "bin/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fand/async-node"
  },
  "scripts": {
    "build": "cross-env BABEL_ENV=production babel src --out-dir lib",
